#include "main.h"
/**
 * print_remaining_days - takes a date and prints how many days are
 * left in the year, taking leap years into account
 * @month: month in number format
 * @day: day of month
 * @year: year
 * Return: void
 */

int _isupper(int c)
{
  if (c >= 65 && c <= 90)
    {
      /* code */
      return (1);
    }
  else
    {
      return (0);
    }
}
